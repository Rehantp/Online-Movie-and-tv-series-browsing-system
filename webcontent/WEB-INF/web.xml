<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

<!-- Admin -->
	<servlet>
		<servlet-name>adminLogin</servlet-name>
		<servlet-class>com.admin.adminLoginServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>adminLogin</servlet-name>
		<url-pattern>/adminLog</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>adminRegister</servlet-name>
		<servlet-class>com.admin.adminRegisterServelet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>adminRegister</servlet-name>
		<url-pattern>/adminReg</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>adminUpdate</servlet-name>
		<servlet-class>com.admin.adminUpdateServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>adminUpdate</servlet-name>
		<url-pattern>/adminUpd</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>adminDelete</servlet-name>
		<servlet-class>com.admin.adminDeleteServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>adminDelete</servlet-name>
		<url-pattern>/adminDel</url-pattern>
	</servlet-mapping>
	<!-- Admin End-->


<!-- reviewer -->	
	<servlet>
		<servlet-name>Reviewer_Login</servlet-name>
		<servlet-class>com.Reviewer.Reviewer_Login_Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reviewer_Login</servlet-name>
		<url-pattern>/reviewer_login</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Reviewer_insert</servlet-name>
		<servlet-class>com.Reviewer.Reviewer_insert</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reviewer_insert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Reviewer_update</servlet-name>
		<servlet-class>com.Reviewer.Reviewer_Update_Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reviewer_update</servlet-name>
		<url-pattern>/updateR</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Reviewer_Delete</servlet-name>
		<servlet-class>com.Reviewer.Reviewer_Delete_Servelet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Reviewer_Delete</servlet-name>
		<url-pattern>/deleteR</url-pattern>
	</servlet-mapping>
	<!-- reviewer end-->
	
	
	<!-- User -->
	 <servlet>
      
      <servlet-name>login</servlet-name>
      <servlet-class>com.User.UserLoginServlet</servlet-class>
      
      </servlet>
      
      <servlet-mapping>
             <servlet-name>login</servlet-name>
             <url-pattern>/login</url-pattern>
      </servlet-mapping> 
      
      <servlet>
      <servlet-name>updateuser</servlet-name>
      <servlet-class>com.User.UpdateuserServlet</servlet-class>
      </servlet>
      <servlet-mapping>
        <servlet-name>updateuser</servlet-name>
        <url-pattern>/update</url-pattern>
      </servlet-mapping>
      
      <servlet>
         <servlet-name>insert </servlet-name>
         <servlet-class>com.User.RegisterUser</servlet-class>
         </servlet> 
         
         <servlet-mapping>
         <servlet-name>insert</servlet-name>
         <url-pattern>/reg</url-pattern>
         </servlet-mapping>
         
         
         <servlet>
          <servlet-name>deleteuser</servlet-name>
          <servlet-class>com.User.DeleteUser</servlet-class>
         </servlet>
      
         <servlet-mapping>
         <servlet-name>deleteuser</servlet-name>
         <url-pattern>/del</url-pattern>
         </servlet-mapping>
      <!-- User End-->   
         
		<!-- Review --> 
         <servlet>
          <servlet-name>review</servlet-name>
          <servlet-class>com.review.ReviewerServlet</servlet-class>
         </servlet>
      
         <servlet-mapping>
         <servlet-name>review</servlet-name>
         <url-pattern>/rev</url-pattern>
         </servlet-mapping>
         
         <!-- Review End--> 
         
         <!-- Agent -->
         <servlet>
		<servlet-name>loginA</servlet-name>
		<servlet-class>com.Agent.LoginServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>loginA</servlet-name>
			<url-pattern>/loginA</url-pattern>
		</servlet-mapping>
		
		
	    <servlet>
	    	<servlet-name>Agentinsert</servlet-name>
	    	<servlet-class>com.Agent.AgentInsert</servlet-class>
	    </servlet>
	    
	    <servlet-mapping>
	    	<servlet-name>Agentinsert</servlet-name>
	    	<url-pattern>/insertA</url-pattern>
	    </servlet-mapping>
	
	
		<servlet>
			<servlet-name>updateAgent</servlet-name>
			<servlet-class>com.Agent.UpdateAgentServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>updateAgent</servlet-name>
			<url-pattern>/updateA</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
			<servlet-name>deleteAgent</servlet-name>
			<servlet-class>com.Agent.DeleteAgentServlet</servlet-class>
		</servlet>
		
		<servlet-mapping>
			<servlet-name>deleteAgent</servlet-name>
			<url-pattern>/deleteA</url-pattern>
		</servlet-mapping>
         <!-- Agent End -->
</web-app>